{
  "name": "Tarot",
  "description": "Jouer Tarot sur ligne",
  "dependencies": {
    "classnames": "~2.2.5",
    "compression": "^1.7.1",
    "connect": "^3.6.5",
    "emoji": "^0.3.2",
    "es6-shim": "~0.35.3",
    "prop-types": "^15.6.0",
    "react": "~16.2.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-document-title": "~2.0.3",
    "react-dom": "~16.2.0",
    "send": "^0.16.1",
    "systemd-socket": "0.0.0",
    "uuid": "^3.1.0",
    "websocket": "^1.0.25"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-plugin-transform-react-constant-elements": "~6.23.0",
    "babel-plugin-transform-react-inline-elements": "~6.22.0",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-0": "~6.24.1",
    "babelify": "~8.0.0",
    "browserify": "~14.5.0",
    "envify": "~4.1.0",
    "eslint": "~4.14.0",
    "eslint-plugin-react": "~7.5.1",
    "fsmonitor": "~0.2.4",
    "less": "~2.7.3",
    "less-plugin-autoprefix": "~1.5.1",
    "less-plugin-clean-css": "~1.5",
    "less-plugin-npm-import": "~2.1.0",
    "normalize.css": "~7.0.0",
    "npm-check-updates": "~2.14.0",
    "uglify-js": "~3.3.0",
    "vinyl-source-stream": "~2.0.0",
    "watchify": "~3.9.0"
  },
  "scripts": {
    "start": "npm run watch-js",
    "watch-js": "watchify -o build-debug/main.js -v -d src/app/main.js",
    "build": "npm run build-js",
    "build-js": "NODE_ENV=production browserify src/app/main.js -t [ babelify --presets [ es2015 stage-0 react ] --plugins [ transform-react-inline-elements transform-react-constant-elements ]] -t [envify] | uglifyjs -cm > build/main.js",
    "updatepackages": "npm-check-updates -u -a",
    "buildServer": "babel src -d lib",
    "startServer": "node lib/server/main.js",
    "buildStartServer": "npm run buildServer && npm run startServer",
    "startProductionServer": "NODE_ENV=production npm run startServer"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "stage-0",
            "react"
          ]
        }
      ],
      "envify"
    ]
  }
}
