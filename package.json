{
  "name": "Tarot",
  "description": "Jouer Tarot sur ligne",
  "dependencies": {
    "classnames": "~2.2.1",
    "compression": "^1.6.1",
    "connect": "^3.4.1",
    "emoji": "^0.3.2",
    "es6-shim": "~0.34.0",
    "react": "~0.14.5",
    "react-addons-shallow-compare": "^0.14.5",
    "react-document-title": "~2.0.1",
    "react-dom": "~0.14.5",
    "send": "^0.13.1",
    "systemd-socket": "0.0.0",
    "uuid": "^2.0.1",
    "websocket": "^1.0.22"
  },
  "devDependencies": {
    "babel": "~6.3.26",
    "babel-cli": "~6.3.17",
    "babel-plugin-transform-react-constant-elements": "~6.3.13",
    "babel-plugin-transform-react-inline-elements": "~6.3.13",
    "babel-preset-es2015": "~6.3.13",
    "babel-preset-react": "~6.3.13",
    "babel-preset-stage-0": "~6.3.13",
    "babelify": "~7.2.0",
    "browserify": "~12.0.1",
    "envify": "~3.4.0",
    "eslint": "~1.10.3",
    "eslint-plugin-react": "~3.13.1",
    "fsmonitor": "~0.2.4",
    "less": "~2.5.3",
    "less-plugin-autoprefix": "~1.5.1",
    "less-plugin-clean-css": "~1.5",
    "less-plugin-npm-import": "~2.1.0",
    "normalize.css": "~3.0.3",
    "npm-check-updates": "~2.5.6",
    "uglify-js": "~2.6.1",
    "vinyl-source-stream": "~1.1.0",
    "watchify": "~3.6.1"
  },
  "scripts": {
    "start": "npm run watch-js",
    "watch-js": "watchify -o build-debug/main.js -v -d src/app/main.js",
    "build": "npm run build-js",
    "build-js": "NODE_ENV=production browserify src/app/main.js -t [ babelify --presets [ es2015 stage-0 react ] --plugins [ transform-react-inline-elements transform-react-constant-elements ]] -t [envify] | uglifyjs -cm > build/main.js",
    "updatepackages": "npm-check-updates -u -a",
    "buildServer": "babel src -d lib",
    "startServer": "node lib/server/main.js",
    "buildStartServer": "npm run buildServer && npm run startServer",
    "startProductionServer": "NODE_ENV=production npm run startServer"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "stage-0",
            "react"
          ]
        }
      ],
      "envify"
    ]
  }
}
